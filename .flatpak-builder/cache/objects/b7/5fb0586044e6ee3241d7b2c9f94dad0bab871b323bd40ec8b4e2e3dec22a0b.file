[Application]
name=com.github.saidbakr.quick-shutdown
runtime=io.elementary.Platform/x86_64/daily
sdk=io.elementary.Sdk/x86_64/daily
command=com.github.saidbakr.quick-shutdown

[Context]
shared=ipc;
sockets=x11;wayland;session-bus;system-bus;fallback-x11;
filesystems=home;

[Extension com.github.saidbakr.quick_shutdown.Debug]
directory=lib/debug
autodelete=true
no-autodownload=true

[Build]
built-extensions=com.github.saidbakr.quick_shutdown.Debug;